{
    "name": "spatie/ssl-certificate-chain-resolver",
    "description": "SSL certificate chain resolver",
    "keywords": [
        "ssl-certificate-chain-resolver",
        "ssl",
        "certificate",
        "chain",
        "https"
    ],
    "homepage": "https://github.com/spatie/ssl-certificate-chain-resolver",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~5.3.1|~6.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/console": "^3.2",
        "tightenco/collect": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\CertificateChain\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\CertificateChain\\Test\\": "tests"
        }
    },
    "bin": [
        "ssl-certificate-chain-resolver"
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
